@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix voc: <https://gestionsalledessports.univ-paris8.fr/onto/voc#> .

<https://gestionsalledessports.univ-paris8.fr/onto/voc> a owl:Ontology ;
    dcterms:title "Getion d'une salle des sports Paris 8.2" .

# Définition du vocabulaire
voc: a owl:Ontology ;
    dcterms:title "Vocabulaire de gestion d'une salle des sprorts"@fr ;
    dcterms:description "Vocabulaire pour la gestion des abonnes et leurs informations générales"@fr .

# Classe abonne
voc:abonne a rdfs:Class ;
    rdfs:label "Abonne"@fr ;
    rdfs:comment "Personne inscrite au programme"@fr .

# Classe InformationsGenerales
voc:InformationsGenerales a rdfs:Class ;
    rdfs:label "Informations Générales"@fr ;
    rdfs:comment "Informations générales liées à un abonne"@fr .

# Propriétés de la classe abonne
voc:nom a rdf:Property ;
    rdfs:label "Nom"@fr ;
    rdfs:domain voc:abonne .

voc:prenom a rdf:Property ;
    rdfs:label "Prénom"@fr ;
    rdfs:domain voc:abonne .

voc:email a rdf:Property ;
    rdfs:label "Email"@fr ;
    rdfs:domain voc:abonne .

voc:dateNaissance a rdf:Property ;
    rdfs:label "Date de naissance"@fr ;
    rdfs:domain voc:abonne .

voc:sexe a rdf:Property ;
    rdfs:label "Sexe"@fr ;
    rdfs:domain voc:abonne .

voc:telephone a rdf:Property ;
    rdfs:label "Numéro de téléphone"@fr ;
    rdfs:domain voc:abonne .

voc:codePostal a rdf:Property ;
    rdfs:label "Code postal"@fr ;
    rdfs:domain voc:abonne .

# Propriétés de la classe InformationsGenerales
voc:hasAbonne a rdf:Property ;
    rdfs:label "A pour abonne"@fr ;
    rdfs:domain voc:InformationsGenerales ;
    rdfs:range voc:abonne .

voc:typeAbonnement a rdf:Property ;
    rdfs:label "Type d'abonnement"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:objectifs a rdf:Property ;
    rdfs:label "Objectifs de remise en forme"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:activitesPreferees a rdf:Property ;
    rdfs:label "Activités préférées"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:blessuresLimitations a rdf:Property ;
    rdfs:label "Avez-vous des blessures ou des limitations physiques ?"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:niveauActivite a rdf:Property ;
    rdfs:label "Niveau d'activité physique actuel"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:taille a rdf:Property ;
    rdfs:label "Taille (en cm)"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:poids a rdf:Property ;
    rdfs:label "Poids (en kg)"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:suiviProgression a rdf:Property ;
    rdfs:label "Suivi de progression"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:horairesPreferes a rdf:Property ;
    rdfs:label "Horaires préférés"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:modePaiement a rdf:Property ;
    rdfs:label "Mode de paiement"@fr ;
    rdfs:domain voc:InformationsGenerales .


voc:planNutritionnel a rdf:Property ;
    rdfs:label "Souhaitez-vous recevoir un plan nutritionnel personnalisé ?"@fr ;
    rdfs:domain voc:InformationsGenerales .

voc:wantWorkoutPlan a rdf:Property ;
    rdfs:label "Souhaitez-vous un programme d'entraînement personnalisé ? " ;
    rdfs:domain voc:InformationsGenerales .

voc:preferredWorkoutTimes a rdf:Property ;
    rdfs:label "Horaires préférés pour s'entraîner " ;
    rdfs:domain voc:InformationsGenerales .

voc:wantWorkoutReminders a rdf:Property ;
    rdfs:label "Souhaitez-vous recevoir des rappels d'entraînement? " ;
    rdfs:domain voc:InformationsGenerales .

voc:preferredPaymentMethod a rdf:Property ;
    rdfs:label "Mode de paiement préféré" ;
    rdfs:domain voc:InformationsGenerales .

voc:promoCode a rdf:Property ;
    rdfs:label "Avez-vous un code promotionnel ? " ;
    rdfs:domain voc:InformationsGenerales .

voc:PreferedHoures a rdf:Property ;
    rdfs:label "Horaires préférés pour s'entraîner" ;
    rdfs:domain voc:InformationsGenerales .
